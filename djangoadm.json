{
    "builds": [
        {
            "src": "vercel_app/wsgi.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "vercel_app/wsgi.py"
        }
    ],
    "DISABLE_COLLECTSTATIC": {
        "description": "Don't run python manage.py collectstatic --no input on deploy",
        "value": "1"
    }
}